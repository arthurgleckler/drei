{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "drei",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "build": {
    "frontendDist": "../src"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "identifier": "com.drei.app",
  "plugins": {
    "app": {
      "enabled": true
    },
    "cli": {
      "args": [
        { "name": "url", "takesValue": true }
      ]
    }
  },
  "productName": "drei",
  "version": "0.1.0"
}